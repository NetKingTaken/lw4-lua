local username_true = {}
local username = globals.get_username()
    username_true['NetKing'] = true
    username_true['EhNoT'] = true
    username_true['Ivo'] = true
    username_true['Couz'] = true


local function sendtrue()
    if username_true[username] == true then
      return true
    else
      return false
    end
end

return sendtrue()
